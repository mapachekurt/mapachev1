# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# A2A Registry Configuration
A2A_REGISTRY_URL=http://localhost:8080
A2A_REGISTRY_TOKEN=dev-token-change-in-production

# Authentication
API_SECRET_KEY=your-secret-key-here-change-in-production
BEARER_TOKEN=dev-bearer-token-change-in-production

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/a2a_registry
# Use in-memory for development
USE_IN_MEMORY_DB=true

# Service Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO

# Agent Configuration
DEFAULT_MODEL=gemini-2.0-flash-exp
AGENT_BASE_URL=http://localhost
